<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  
<mapper namespace="com.tm.s5.user.UserDAO">
	<select id="memberLogin" parameterType="com.tm.s5.user.UserVO" resultType="com.tm.s5.user.UserVO">
		select * from userInfo where id=#{id} and pwd=#{pwd}
	</select>

</mapper>